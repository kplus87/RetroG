{
"name":"RetroG",
"description":"Search, the way it used to be.",
"version":"0.0.0.2",
"manifest_version":3,
"content_scripts": [
    {
      "matches": ["http://www.google.com/*","https://www.google.com/*"],
      "js": ["jquery.min.js","retrog.js"]
    }
  ],
 "icons": {
        "16": "./images/icon-16.png",
        "32": "./images/icon-32.png",
        "48": "./images/icon-48.png",
        "128": "./images/icon-128.png"
    },
   "action": {
        "default_popup": "RetroG.html",
        "default_icons": {
            "16": "./images/icon-16.png",
            "32": "./images/icon-32.png",
            "48": "./images/icon-48.png",
            "128": "./images/icon-128.png"
        },
"permissions": [
   "activeTab",
   "storage"
   ]
}
}